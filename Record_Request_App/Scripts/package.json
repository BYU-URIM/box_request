{
  "name": "byu-records-center",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:local": "webpack-dev-server --env.NODE_ENV=local --mode=development --hints=off",
    "dev:sharepoint": " npm run sp:proxyServer & webpack --watch --mode=development --env.NODE_ENV=sharepoint",
    "dev:sharepointProxy": "npm run fmp:serve & npm run sp:proxyServer & webpack --watch --env.NODE_ENV=sharepointProxy --mode=development",
    "build": "webpack --env.NODE_ENV=prod --mode=production",
    "build:production": "webpack --env.Node_ENV=sharepoint --optimize-minimize",
    "test": "jest",
    "lint": "npm run lint:styles & npm run lint:typescript",
    "lint:typescript": "ts-node --project ./tsconfig.test.json ./tasks/lint-typescript.ts",
    "lint:styles": "ts-node --project ./tsconfig.test.json ./tasks/lint-styles.ts",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "sp:proxyServer": "node ./server/sp-server.js",
    "fmp:serve": "node ./server/fmp-server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.4",
    "@types/react-jsonschema-form": "^1.0.10",
    "@types/request": "^2.47.1",
    "@types/shelljs": "^0.8.0",
    "@types/storybook-addon-jsx": "^5.4.3",
    "@types/storybook__addons": "^4.1.0",
    "@types/storybook__react": "^4.0.1",
    "@types/webpack": "^4.4.29",
    "babel-loader": "^8.0.5",
    "cors": "^2.8.4",
    "css-loader": "^0.28.11",
    "docz": "^0.11.2",
    "docz-core": "^0.11.2",
    "express": "^4.16.3",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "husky": "^0.14.3",
    "jest": "^24.7.1",
    "mobx-react-devtools": "^6.0.2",
    "node-sass": "^4.9.0",
    "prettier": "^1.14.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "request": "^2.88.0",
    "sass-loader": "^7.0.3",
    "shelljs": "^0.8.2",
    "source-map-loader": "^0.2.3",
    "sp-rest-proxy": "^2.7.4",
    "storybook-addon-jsx": "^7.1.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.5.0",
    "ts-jest": "^24.0.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.5.2",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.4.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@pnp/common": "^1.2.1",
    "@pnp/logging": "^1.2.1",
    "@pnp/odata": "^1.2.1",
    "@pnp/sp": "^1.2.1",
    "@pnp/sp-addinhelpers": "^1.2.1",
    "@uifabric/fluent-theme": "^0.16.6",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.3",
    "office-ui-fabric-react": "^7.0.0-alpha018",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-jsonschema-form": "^1.0.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}